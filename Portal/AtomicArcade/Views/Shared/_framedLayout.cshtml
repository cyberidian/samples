<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Atomic Arcade - Cyberidian Digital Arts</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <script type="text/javascript">
    </script>
</head>
<body class="bg-main">
    @Html.Partial("_navigationPartial")
    <div class="container bg-main-container ">
        <div class="row text-center">
            <div class="col-sm-12 col-md-12 col-lg-12 col-xs-6">
                <div class="well well-frame">
                    <div class="well well-innerframe">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
                    @Html.Partial("_footerPartial")
                    @Scripts.Render("~/bundles/jquery")
                    @Scripts.Render("~/bundles/bootstrap")
                    @RenderSection("scripts", required: false)
</body>
</html>
